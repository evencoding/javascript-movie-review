{
  "compilerOptions": {
    "target": "ES2015",

    "module": "ESNext",
    "moduleResolution": "node",

    "allowJs": true,
    "removeComments": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": false,

    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "skipLibCheck": true
  }
}
